<templateSet group="Custom">
  <template name="vapi" value="api.$START$()&#10;.then(res =&gt; {&#10;    &#10;  })&#10;  .catch(err =&gt; {&#10;    &#10;  })" shortcut="TAB" description="Vue Api Call" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="clog" value="console.log($START$);" description="Console Log" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="pfunc" value="public function $START$($VAR1$)&#10;{   &#10;$END$&#10;}&#10;" description="public function" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="bpfnc" value="$fields = [&#10;&#10;$END$&#10;&#10;];&#10;&#10;$columns = [&#10;&#10;&#10;&#10;];" description="Create $fields s, $columns arrays" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="foreignkey" value="$table-&gt;foreign('$START$')&#10;    -&gt;references('id')&#10;    -&gt;on('$END$')&#10;    -&gt;onDelete('cascade');" description="" toReformat="true" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="catch" value="try{&#10;$START$&#10;&#10;}catch ($EXCEPTION$ $e){&#10;$END$&#10;}&#10;" description="Try Catch" toReformat="true" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="EXCEPTION" expression="" defaultValue="&quot;Exception&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="dd" value="dd($START$);" description="Die And Dump" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="AS" value="{{asset('$SELECTION$')}}" description="Surround with laravel asset()" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="RT" value="{{route('$SELECTION$')}}" description="Surround with laravel route()" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="site.title" value="{{setting('site.title')}}" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="site.phone" value="{{setting('site.phone')}}" description="" toReformat="false" toShortenFQNames="true" />
  <template name="setp" value="{{setting('site.phone')}}" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="sett" value="{{setting('site.title')}}" description="Settings Title" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="ph" value="{{phone(config('website.phone'), 'US')-&gt;formatNational()}}" description="Phone" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="tel" value="{{phone(config('website.phone'), 'US')-&gt;formatRFC3966()}}" description="Telephone Link" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="mail" value="{{config('website.email')}}" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
</templateSet>